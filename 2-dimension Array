#include<stdio.h>
int main()
{
    int a[3][2]={{5,0},{1,2},{2,3}};
    int b[3][2]={{1,2},{2,3},{3,6}};
    int c[3][2];
    int i,j;    
        
    for(i=0;i<3;i++)
    {
        for(j=0;j<2;j++)
        {
            c[i][j]=a[i][j]+b[i][j];
            printf("%d\t",c[i][j]);
        
             if(j==1)
                printf("\n");
        }
     }
     
    return 0;
}


-------------------------------------------------------------------------------------------------

Output--

6       2
3       5
5       9
