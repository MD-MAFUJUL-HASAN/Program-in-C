#include<stdio.h>
int main()
{
 int marks;

 printf("Enter your marks: ");
 scanf("%d",&marks);

 if(marks>=40)
   {
   printf("You are a winner\n");
   }

 else
   {
   printf("you are a loser\n");
   }

   return 0;
}


--------------------------------------------------------------

output--

Enter your marks: 60
You are a winner
