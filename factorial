#include<stdio.h>
int main()
{
    int a=1,num;
    
    printf("Enter a number: ");
    scanf("%d",&num);
    
    while(num>=1)
        {
            a = (a*num);
            num--;
        }
        
    printf("Factorial is = %d",a);
    
    return 0;
}


------------------------------------------------

output--

Enter a number: 5
Factorial is = 120
