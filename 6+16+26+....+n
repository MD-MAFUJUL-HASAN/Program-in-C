#include<stdio.h>
int main ()
{
    int i,n,sum=0;

    printf("The value of n: ");
    scanf("%d",&n);

    for(i=6;i<=n;i=i+10)
        sum=sum+i;

    printf("\nSummation is = %d",sum);

    return 0;
}


