#include<stdio.h>
int main()
{
    int i,j,c=0;
    int a[]={216,125,64,27,8,1};
    for(i=3;i>0;i--)
    {
        for(j=0;j<i;j++)
        {
            printf("%d ",a[c]);
            c++;
        }
        printf("\n");
    }
    return 0;
}


----------------------------------------------------

output--

216 125 64
27 8
1
