#include<stdio.h>
int main()
{
    int i,size=100,a[100],n;
    printf("\nEnter array Elements: ");
    for(i=0;i<size;i++)
    {
        scanf("%d\n",&a[i]);
    }
    for(i=0;i<size;i++)
    {
        if(a[i]%2==0)
        {
            printf("Even number = %d\n",a[i]);
        }
    }
    return 0;
}
